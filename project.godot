; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="CyberBlood"
config/tags=PackedStringArray("game", "2d")
run/main_scene="res://scenes/ui/main_menu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="CyberBlood"
config/features=PackedStringArray("4.1", "Forward Plus")
config/icon="res://icon.svg"
config/version="0.0.0.9"

[autoload]

Logger="*res://addons/bloody_logger/logger.gd"
Settings="*res://scripts/settings.gd"
GameManager="*res://scripts/game_manager.gd"
Database="*res://scripts/database.gd"
EnemyManager="*res://scripts/enemy_manager.gd"

[debug]

gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/size/resizable=false
window/size/borderless=true
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"

[editor]

naming/node_name_casing=1

[editor_plugins]

enabled=PackedStringArray("res://addons/AS2P/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg", "res://addons/bloody_logger/plugin.cfg", "res://addons/godot-sqlite/plugin.cfg")

[filesystem]

import/blender/enabled=false

